{
	"name": "bs5",
	"displayName": "Bootstrap5",
	"description": "Bootstrap5 snippets",
	"version": "0.1.0",
	"author": "Ajanuw",
	"icon": "logo/bootstrap-logo.png",
	"publisher": "ajanuw",
	"homepage": "https://github.com/januwA/Bootstrap5-snippets",
	"repository": {
		"type": "git",
		"url": "https://github.com/januwA/Bootstrap5-snippets.git"
	},
	"keywords": [
		"Bootstrap5 snippets",
		"Bootstrap",
		"Bootstrap5",
		"snippets",
		"Ajanuw"
	],
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Snippets"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "html",
				"path": "./snippets/bs5/snippets.json"
			},
			{
				"language": "html",
				"path": "./snippets/bs5/class.json"
			},
			{
				"language": "scss",
				"path": "./snippets/bs5/scss.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.15.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	}
}